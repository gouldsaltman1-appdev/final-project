<div class="card">
  <div class="card-header">
    <h3 class="card-title">
      All Users
    </h3>
  </div>

  <ul class="list-group">
    <% @users.each do |user| %>
      <li class="list-group-item">
        <a href="/users/<%= user.id %>">
          <%= user.username %>
        </a>
      </li>
    <% end %>

  </ul>
</div>
